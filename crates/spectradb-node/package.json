{
  "name": "tensordb",
  "version": "0.2.0",
  "description": "AI-native, bitemporal ledger database with MVCC, SQL, and LSM storage",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "tensordb",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "license": "PolyForm-Noncommercial-1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensor-db/tensorDB"
  },
  "keywords": [
    "database",
    "bitemporal",
    "mvcc",
    "embedded",
    "ledger",
    "sql",
    "napi-rs"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "tensordb.*.node"
  ],
  "scripts": {
    "build": "napi build --release --platform",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  }
}
